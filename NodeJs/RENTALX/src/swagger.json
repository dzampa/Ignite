{
    "openapi": "3.0.0",
    "info":{
        "title": "RentalX Documentation",
        "description": "This is as API Rent",
        "version": "1.0.0",
        "contact": {
            "email": "diego@tx.xom"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": ["category"],
                "summary": "Create a category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "category name sample",
                                    "description": "category description sample"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Category already exists"
                    }
                }
            },
            "get": {
                "tags": ["category"],
                "summary": "list all category",
                "description": "List all category",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }

            }
        },
        "/categories/import":{
            "post":{
                "tags":["category"],
                "summary":"Upload a new category",
                "requestBody":{
                    "content":{
                        "multipart/form-data":{
                            "schema":{
                                "type": "object",
                                "properties":{
                                    "file":{
                                        "type":"string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    }
                }
            }
        },
        "/specifications":{
            "post":{
                "tags": ["specifications"],
                "summary": "Create a specification",
                "requestBody": {
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref": "#/definitions/Specification"
                            }
                        }
                    }
                }
            },
            "response":{
                "201":{
                    "description":"Create"
                },
                "500":{
                    "description":"Specifications already exists"
                }
            },
            "get": {
                "tags": ["specifications"],
                "summary": "list all specification",
                "description": "List all specification",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
    
            }
        }
    },
    "definitions": {
        "Specification":{
            "type": "object",
            "properties":{
                "name":{
                    "type":"string"
                },
                "description":{
                    "type":"string"
                }
            }
        }
    }
}